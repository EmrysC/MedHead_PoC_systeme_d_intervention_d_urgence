                          "// 1. Vérification du code de statut HTTP (400 car c'est une erreur de saisie client)\r",
                          "pm.test(\"Statut attendu : 400 Bad Request\", function () {\r",
                          "    pm.response.to.have.status(400);\r",
                          "});\r",
                          "\r",
                          "// 2. Vérification de la précision du diagnostic technique\r",
                          "pm.test(\"L'API identifie précisément l'erreur de type sur la latitude\", function () {\r",
                          "    var jsonData = pm.response.json();\r",
                          "    \r",
                          "    // On vérifie le message principal\r",
                          "    pm.expect(jsonData.message).to.eql(\"Format de paramètre incorrect\");\r",
                          "    \r",
                          "    // On vérifie que c'est bien le champ 'latitude' qui a posé problème\r",
                          "    pm.expect(jsonData.errors).to.have.property(\"latitude\");\r",
                          "    \r",
                          "    // On vérifie que le message explique bien le problème (insensible à la casse si besoin)\r",
                          "    var errorDetail = jsonData.errors.latitude;\r",
                          "    pm.expect(errorDetail).to.include(\"latitude\");\r",
                          "    pm.expect(errorDetail).to.include(\"Double\");\r",
                          "    pm.expect(errorDetail).to.include(\"valeur invalide\");\r",
                          "});\r",
                          "\r",
                          "// 3. Validation de la conformité au contrat d'interface ErrorDetails\r",
